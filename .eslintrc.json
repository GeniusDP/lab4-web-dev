{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:sonarjs/recommended"],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "plugins": ["sonarjs"],
    "rules": {
        "sonarjs/cognitive-complexity": "error",
        "sonarjs/no-identical-expressions": "error"
    }
}
